{
  "name": "iov-data-collector",
  "version": "1.0.0",
  "description": "BrightData data collection worker for IOV Viral SaaS",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "set NODE_ENV=production && node dist/index.js",
    "service:install": "npm run build && node scripts/install-service.js",
    "service:uninstall": "node scripts/install-service.js uninstall"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bullmq": "^5.1.0",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "node-windows": "^1.0.0-beta.8",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
